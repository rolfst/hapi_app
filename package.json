{
  "name": "hapi-test",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "migrate": "sequelize db:migrate",
    "start:acc": "NODE_PATH=build NODE_ENV=acceptance node build/index.js",
    "clean": "rimraf build && mkdir build",
    "copy-config": "cp ./source/database.json ./build/",
    "build": "npm run clean && babel ./source -d ./build -s && npm run copy-config",
    "dev": "cross-env NODE_PATH=source NODE_ENV=development babel-node ./source/index.js",
    "test": "cross-env NODE_PATH=source NODE_ENV=development mocha --compilers js:babel-register tests/**/*.js",
    "flow": "flow --help",
    "lint": "eslint source/",
    "check": "npm run lint && npm run test && npm outdated --depth 0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bluebird": "3.3.4",
    "boom": "3.1.2",
    "hapi": "13.2.1",
    "joi": "8.0.4",
    "jwt-simple": "0.5.0",
    "lodash": "4.6.1",
    "mysql": "2.10.2",
    "sequelize": "3.19.3"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babel-register": "6.7.2",
    "chai": "3.5.0",
    "cross-env": "1.0.7",
    "eslint": "2.3.0",
    "eslint-config-airbnb": "6.1.0",
    "eslint-plugin-react": "4.2.1",
    "mocha": "2.4.5",
    "rimraf": "^2.5.2",
    "sequelize-cli": "2.3.1"
  }
}
