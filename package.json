{
  "name": "hapi-test",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "migrate": "sequelize db:migrate",
    "clean": "rimraf build && mkdir build",
    "copy-config": "cp ./source/database.json ./build/",
    "start:test": "cross-env NODE_PATH=build NODE_ENV=development node build/index.js",
    "start:acc": "cross-env NODE_PATH=build NODE_ENV=acceptance node build/index.js",
    "start:prod": "cross-env NODE_PATH=build NODE_ENV=production node build/index.js",
    "build": "npm run clean && babel ./source -d ./build -s && npm run copy-config",
    "debug": "node-debug | npm run dev",
    "dev": "cross-env NODE_PATH=source NODE_ENV=development babel-node ./source/index.js",
    "test": "npm run test:e2e && npm run test:unit",
    "test:e2e": "cross-env NODE_PATH=source NODE_ENV=testing mocha --timeout 10000 --compilers js:babel-register source/common/test-utils/setup.js source/**/*.e2e.js",
    "test:unit": "cross-env NODE_PATH=source NODE_ENV=testing mocha --compilers js:babel-register source/common/test-utils/setup.js source/**/*.spec.js",
    "coverage": "cross-env NODE_PATH=source NODE_ENV=testing istanbul cover _mocha -- --compilers js:babel-register './source/**/*.spec.js'",
    "flow": "flow --help",
    "lint": "eslint source/",
    "check": "npm run lint && npm run test && npm outdated --depth 0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "bluebird": "^3.3.4",
    "boom": "3.1.2",
    "dotenv": "2.0.0",
    "hapi": "13.2.1",
    "isomorphic-fetch": "2.2.1",
    "joi": "8.0.4",
    "jwt-simple": "0.5.0",
    "lodash": "4.6.1",
    "moment": "2.12.0",
    "mysql": "2.10.2",
    "node-fetch": "^1.5.1",
    "node-inspector": "0.12.7",
    "parse": "1.8.1",
    "sequelize": "3.19.3",
    "superagent": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babel-register": "6.7.2",
    "chai": "3.5.0",
    "cross-env": "1.0.7",
    "eslint": "2.3.0",
    "eslint-config-airbnb": "6.1.0",
    "eslint-plugin-react": "4.2.1",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "2.4.5",
    "rimraf": "2.5.2",
    "sequelize-cli": "2.3.1"
  }
}
