  "main": "index.js",
  "scripts": {
    "start:test": "cross-env NODE_ENV=production API_ENV=development node ./source/index.js",
    "start:acc": "cross-env NODE_ENV=production API_ENV=acceptance node ./source/index.js",
    "start:prod": "cross-env NODE_ENV=production API_ENV=production node ./source/index.js",
    "start:cron": "cross-env NODE_ENV=production API_ENV=production node ./source/modules/scheduler/scheduler.js",
    "seed:demo": "cross-env NODE_ENV=acceptance API_ENV=acceptance node ./scripts/seed-demo-environment",
    "dev": "cross-env NODE_ENV=debug API_ENV=development node ./source/index.js",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "API_ENV=testing mocha --opts ./mocha.opts source/**/*.e2e.js",
    "test:unit": "API_ENV=testing mocha --opts ./mocha-spec.opts source/**/*.spec.js",
    "coverage": "cross-env API_ENV=testing istanbul cover _mocha -- source/shared/test-utils/spec-setup.js './source/**/*.spec.js' -s 10",
    "flow": "flow --help",
    "lint": "eslint source/",
    "doc": "jsdoc -c ./jsdoc.json",
    "check": "npm run lint && npm run test"
  },
