{
  "name": "node-flex-api",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build && mkdir build",
    "start:test": "cross-env NODE_PATH=build NODE_ENV=production API_ENV=development node build/index.js",
    "start:acc": "cross-env NODE_PATH=build NODE_ENV=production API_ENV=acceptance node build/index.js",
    "start:prod": "cross-env NODE_PATH=build NODE_ENV=production API_ENV=production node build/index.js",
    "build": "npm run clean && babel ./source -d ./build -s --copy-files --ignore spec.js,e2e.js",
    "debug": "node-debug | npm run dev",
    "dev": "cross-env NODE_PATH=source NODE_ENV=debug API_ENV=development babel-node ./source/index.js",
    "test": "npm run test:unit && npm run test:e2e",
    "test:integrations": "cross-env NODE_PATH=source API_ENV=testing mocha --timeout 20000 --compilers js:babel-register source/shared/test-utils/setup.js 'source/adapters/**/*.e2e.js'",
    "test:e2e": "API_ENV=testing mocha --opts ./mocha.opts source/**/*.e2e.js",
    "test:unit": "API_ENV=testing mocha --opts ./mocha-spec.opts source/**/*.spec.js",
    "coverage": "cross-env NODE_PATH=source API_ENV=testing istanbul cover _mocha -- --compilers js:babel-register source/shared/test-utils/spec-setup.js './source/**/*.spec.js' -s 10",
    "flow": "flow --help",
    "lint": "eslint source/",
    "doc": "jsdoc -c ./jsdoc.json",
    "check": "npm run lint && npm run test && npm outdated --depth 0",
    "jobs": "cross-env NODE_PATH=source NODE_ENV=production babel-node ./source/modules/flexchange/scripts/createRemindersForAdmin.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "2.9.0",
    "bcrypt": "0.8.6",
    "bluebird": "3.3.4",
    "boom": "3.1.2",
    "bunyan": "1.8.4",
    "commander": "2.9.0",
    "dotenv": "2.0.0",
    "hapi": "13.2.1",
    "hapi-async-handler": "1.0.3",
    "intercom-client": "2.8.5",
    "isomorphic-fetch": "2.2.1",
    "joi": "8.0.4",
    "jwt-simple": "0.5.0",
    "lodash": "4.15.0",
    "mixpanel": "0.4.0",
    "moment": "2.12.0",
    "moment-timezone": "0.5.5",
    "mysql": "2.11.1",
    "newrelic": "1.31.0",
    "nodemailer": "2.5.0",
    "parse": "1.8.1",
    "qs": "6.2.1",
    "ramda": "0.23.0",
    "raven": "0.12.1",
    "sequelize": "3.19.3",
    "smtpapi": "1.2.0",
    "uuid-v4": "0.1.0",
    "yargs": "6.3.0"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-eslint": "6.0.4",
    "babel-plugin-transform-async-to-generator": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.6.0",
    "babel-register": "6.7.2",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "cross-env": "1.0.7",
    "eslint": "2.3.0",
    "eslint-config-airbnb": "6.1.0",
    "eslint-plugin-mocha": "4.7.0",
    "eslint-plugin-react": "4.2.1",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "3.1.2",
    "nock": "8.0.0",
    "node-inspector": "0.12.7",
    "rewire": "2.5.2",
    "rimraf": "2.5.2",
    "sequelize-cli": "2.3.1",
    "sinon": "1.17.4"
  }
}
