{
  "source": {
    "include": ["source"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": ".+\\.(spec|e2e)\\.js"
  },
  "plugins": ["node_modules/jsdoc-strip-async-await"],
  "opts": {
    "recurse": true,
    "encoding": "utf8",
    "destination": "./docs"
  },
  "templates": {
    "monospacelinks": true
  }
}
