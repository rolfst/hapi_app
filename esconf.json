{
  "source": "source",
  "destination": "./edocs",
  "includes": ["\\.(js|jsdoc)"],
  "excludes": ["\\.(e2e|spec)\\.js$"],
  "experimentalProposal": {
    "classProperties": true,
    "objectRestSpread": true,
    "decorators": true,
    "doExpressions": true,
    "functionBind": true,
    "asyncGenerators": true,
    "exportExtensions": true,
    "dynamicImport": true
  }
}
